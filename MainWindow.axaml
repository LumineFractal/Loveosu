      <TabItem Name="Settings" Header="Settings" FontSize="24">
        <Canvas HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top">
          <TextBlock Text="User ID to spy on:"
                     Canvas.Left="10" Canvas.Top="15"
                     FontSize="18"
                     HorizontalAlignment="Left" VerticalAlignment="Center"/>
          <TextBox Name="UserID"
                   Canvas.Left="190" Canvas.Top="8"
                   FontSize="18"
                   TextWrapping="Wrap"
                   Width="400"
                   HorizontalAlignment="Left"  VerticalAlignment="Center"/>
          <TextBlock Text="Client ID:"
                     Canvas.Left="10" Canvas.Top="57"
                     FontSize="18"
                     HorizontalAlignment="Left" VerticalAlignment="Center"/>
          <TextBox Name="ClientID"
                   Canvas.Left="190" Canvas.Top="50"
                   FontSize="18"
                   TextWrapping="Wrap"
                   Width="400"
                   HorizontalAlignment="Left"  VerticalAlignment="Center"/>
          <TextBlock Text="Client Secret:"
                     Canvas.Left="10" Canvas.Top="99"
                     FontSize="18"
                     HorizontalAlignment="Left" VerticalAlignment="Center"/>
          <TextBox Name="ClientSecret"
                   PasswordChar="*"
                   Canvas.Left="190" Canvas.Top="92"
                   FontSize="18"
                   TextWrapping="Wrap"
                   Width="400"
                   HorizontalAlignment="Left" VerticalAlignment="Center"/>
          <TextBlock Text="Update every                               seconds"
                     Canvas.Left="10" Canvas.Top="152"
                     FontSize="18"
                     HorizontalAlignment="Left" VerticalAlignment="Center"/>
          <TextBox Name="UpdateInterval"
                   Text="20"
                   Canvas.Left="190" Canvas.Top="144"
                   FontSize="18"
                   TextWrapping="Wrap"
                   Width="80"
                   HorizontalAlignment="Left" VerticalAlignment="Center"/>
          <TextBlock Text="Saved!"
                 Name="SavedTextBlock"
                 IsVisible="False"
                 Foreground="LightGreen"
                 FontSize="20"
                 Canvas.Left="180" Canvas.Top="530"/>
          <TextBlock Text="Wait..."
                 Name="WaitTextBlock"
                 IsVisible="False"
                 Foreground="LightCoral"
                 FontSize="20"
                 Canvas.Left="70" Canvas.Top="530"/>
          <TextBlock Text="Error, check key or username"
                 Name="ErrorTextBlock"
                 IsVisible="False"
                 Foreground="Red"
                 FontSize="20"
                 Canvas.Left="0" Canvas.Top="530"/>
          <Button Content="Save"
                  Click="OnSaveButtonClick"
                  FontSize="20"
                  Canvas.Left="450" Canvas.Top="520"
                  Height="60"
                  Width="120"
                  HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                  HorizontalAlignment="Left" VerticalAlignment="Center"/>
